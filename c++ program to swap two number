#include<iostream>
using namespace std;
int main()
{
	int a,b,temp;
cout<<"enter the valu of a";
cin>>a;
cout<<"enter the valu of  b";
cin>>b;
cout<<"before the swapping"<<endl;
cout<<"a ="<<a<<"b ="<<b<<endl;
temp=a;
a=b;
b=temp;
cout<<"after the swapping"<<endl;
cout<<"a ="<<a<<"b ="<<b<<endl; 

return 0;
}
