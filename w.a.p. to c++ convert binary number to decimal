#include<iostream>
#include<cmath>
using namespace std;
int main()
{
	long long num;
	int decimalnum,i,rem;
	cout <<"enter any num:";
	cin>>num;
	decimalnum=0;
	i=0;
	while(num !=0)
	{
		rem = num%10;
		num /= 10;
		decimalnum += rem* pow(2,i);
		++i;
		
		
	}
	cout<<"equivalent decimal number :"<<decimalnum<<endl;
	return 0;
	
}
